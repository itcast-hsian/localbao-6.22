.navigate-bar{
    padding: 20rpx;
    border-bottom: 1px #eee solid;
    font-weight: bold;
    display: flex;
    justify-content: space-between;
    align-items: center;
}

.navigate-bar::after{
    display: block;
    content: "";
    width: 10px;
    height: 10px;
    border-right: 1px #999 solid;
    border-top:1px #999 solid;
    transform: rotate(45deg);
    margin-right: 5px;
}